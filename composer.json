{
    "name": "absszero/phead",
    "description": "A PHP code generator to generate code via your LAYOUT file.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "generator",
        "code"
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/console": "^5.4 || ^7.0",
        "symfony/yaml": "^5.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0 || ^10.0",
        "vimeo/psalm": "^4.0 || ^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Absszero\\Phead\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": [
        "bin/phead"
    ]
    ,
    "scripts": {
        "static-analysis": [
            "@php ./vendor/bin/phpstan",
            "@php ./vendor/bin/psalm --no-cache"
        ],
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    }
}
